{
  "name": "admin",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "antd": "^2.13.8",
    "ayano-react": "^0.1.10",
    "ayano-script": "^0.1.12",
    "react": "^16.0.0",
    "react-dom": "^16.0.0"
  },
  "scripts": {
    "start": "ayano-script start",
    "build": "ayano-script build",
    "publish": "ayano-script publish"
  },
  "ayano-config": {
    "disablePx2Rem": true,
    "resourcePrefix": "https://dn-mhc.qbox.me/faw/js/",
    "resourceDescribeFileName": "resources.json",
    "antd": [
      "antd"
    ]
  },
  "ayano-publish-config": {
    "type": "1",
    "server": "",
    "appId": "<your app id>",
    "htmlPath": "./index.html",
    "resourceFile": "./resources.json"
  },
  "proxy": {
    "/api": {
      "target": "http://localhost:3001/api",
      "changeOrigin": true,
      "pathRewrite": {
        "^/api": ""
      }
    }
  }
}
